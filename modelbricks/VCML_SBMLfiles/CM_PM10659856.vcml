bp:name><bp:xref rdf:nodeID="uniprot_P04483" /></bp:PhysicalEntity><bp:PublicationXref rdf:nodeID="uniprot_P04483"><bp:db rdf:datatype="http://www.w3.org/2001/XMLSchema#string">UNIPROT</bp:db><bp:id rdf:datatype="http://www.w3.org/2001/XMLSchema#string">P04483</bp:id></bp:PublicationXref><bp:PhysicalEntity rdf:nodeID="BIOMODEL_PX"><bp:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PX</bp:name><bp:xref rdf:nodeID="uniprot_P03023" /></bp:PhysicalEntity><bp:PublicationXref rdf:nodeID="chebi_33699"><bp:db rdf:datatype="http://www.w3.org/2001/XMLSchema#string">CHEBI</bp:db><bp:id rdf:datatype="http://www.w3.org/2001/XMLSchema#string">33699</bp:id></bp:PublicationXref><bp:PublicationXref rdf:nodeID="uniprot_P03023"><bp:db rdf:datatype="http://www.w3.org/2001/XMLSchema#string">UNIPROT</bp:db><bp:id rdf:datatype="http://www.w3.org/2001/XMLSchema#string">P03023</bp:id></bp:PublicationXref><bp:PhysicalEntity rdf:nodeID="BIOMODEL_PZ"><bp:name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PZ</bp:name><bp:xref rdf:nodeID="uniprot_P03034" /></bp:PhysicalEntity></rdf:RDF></pathwayModel><relationshipModel><RMNS version="3.0"><RelationshipObject bioPaxObjectID="BioPaxObject(BIOMODEL_PY)" bioModelObjectID="SpeciesContext(TetR_protein)" /><RelationshipObject bioPaxObjectID="BioPaxObject(BIOMODEL_X)" bioModelObjectID="SpeciesContext(LacI_mRNA)" /><RelationshipObject bioPaxObjectID="BioPaxObject(BIOMODEL_Z)" bioModelObjectID="SpeciesContext(cI_mRNA)" /><RelationshipObject bioPaxObjectID="BioPaxObject(BIOMODEL_PZ)" bioModelObjectID="SpeciesContext(cI_protein)" /><RelationshipObject bioPaxObjectID="BioPaxObject(BIOMODEL_PX)" bioModelObjectID="SpeciesContext(LacI_protein)" /><RelationshipObject bioPaxObjectID="BioPaxObject(BIOMODEL_Y)" bioModelObjectID="SpeciesContext(TetR_mRNA)" /></RMNS></relationshipModel><vcmetadata><rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"><rdf:Description rdf:about="http://sourceforge.net/projects/vcell/vcml/org.vcell.mode