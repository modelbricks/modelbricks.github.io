<bqmodel:isDescribedBy xmlns:bqmodel="http://biomodels.net/model-qualifiers/"><rdf:Bag rdf:nodeID="node13"><rdf:_1><rdf:Description rdf:about="urn:miriam:uniprot:P03023" /></rdf:_1></rdf:Bag></bqmodel:isDescribedBy><bqmodel:isDescribedBy xmlns:bqmodel="http://biomodels.net/model-qualifiers/"><rdf:Bag rdf:nodeID="node14"><rdf:_1><rdf:Description rdf:about="urn:miriam:chebi:33699" /></rdf:_1></rdf:Bag></bqmodel:isDescribedBy></rdf:Description><rdf:Description rdf:about="http://www.sbml.org/sbml/level2/version3/_905781"><bqmodel:isDescribedBy xmlns:bqmodel="http://biomodels.net/model-qualifiers/"><rdf:Bag rdf:nodeID="node15"><rdf:_1><rdf:Description rdf:about="urn:miriam:chebi:33699" /></rdf:_1></rdf:Bag></bqmodel:isDescribedBy><bqmodel:isDescribedBy xmlns:bqmodel="http://biomodels.net/model-qualifiers/"><rdf:Bag rdf:nodeID="node16"><rdf:_1><rdf:Description rdf:about="urn:miriam:uniprot:P04483" /></rdf:_1></rdf:Bag></bqmodel:isDescribedBy><bqmodel:isDescribedBy xmlns:bqmodel="http://biomodels.net/model-qualifiers/"><rdf:Bag rdf:nodeID="node17"><rdf:_1><rdf:Description rdf:about="urn:miriam:kegg.compound:P04483" /></rdf:_1></rdf:Bag></bqmodel:isDescribedBy></rdf:Description><rdf:Description rdf:about="http://www.sbml.org/sbml/level2/version3/_905802"><bqmodel:isDescribedBy xmlns:bqmodel="http://biomodels.net/model-qualifiers/"><rdf:Bag rdf:nodeID="node18"><rdf:_1><rdf:Description rdf:about="urn:miriam:chebi:33699" /></rdf:_1></rdf:Bag></bqmodel:isDescribedBy><bqmodel:isDescribedBy xmlns:bqmodel="http://biomodels.net/model-qualifiers/"><rdf:Bag rdf:nodeID="node19"><rdf:_1><rdf:Description rdf:about="urn:miriam:kegg.compound:C00046" /></rdf:_1></rdf:Bag></bqmodel:isDescribedBy><bqmodel:isDescribedBy xmlns:bqmodel="http://biomodels.net/model-qualifiers/"><rdf:Bag rdf:nodeID="node20"><rdf:_1><rdf:Description rdf:about="urn:miriam:uniprot:P03034" /></rdf:_1></rdf:Bag></bqmodel:isDescribedBy></rdf:Description></rdf:RDF><nonrdfAnnotationList><nonrdfAnnotation vcid="Species(PZ)